  },
  "homepage": "https://github.com/1cgonza/dddrawings",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.18.12",
    "chalk": "^1.1.3",
    "circular-json": "^0.3.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "connect-gzip-static": "^2.0.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
