{
  "name": "ddd",
  "version": "1.0.0",
  "description": "Data Driven Drawings",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/1cgonza/dddrawings"
  },
  "scripts": {
    "dev": "babel-node app.js",
    "pack": "webpack --watch --mode production --progress --colors",
    "start": "concurrently --kill-others \"webpack --watch --mode production --progress --colors\" \"babel-node app.js\"",
    "clean": "babel-node app --clean",
    "build": "babel-node app --prod",
    "deploy": "babel-node app --deploy"
  },
  "keywords": [
    "PhD",
    "Experiments",
    "Data",
    "Drawings"
  ],
